<root>
    <top>/listprojectaccountsresponse/projectaccount</top>
    <command>listProjectAccounts</command>
    <description>Lists project&apos;s accounts</description>
    <defaultresponses>projectid,project,account,domain</defaultresponses>
    <isAsync>false</isAsync>
    <sinceVersion>3.0.0</sinceVersion>
    <params>
        <param>
            <name>projectid</name>
            <description>id of the project</description>
            <required>true</required>
        </param>
        <param>
            <name>account</name>
            <description>list accounts of the project by account name</description>
            <required>false</required>
        </param>
        <param>
            <name>keyword</name>
            <description>List by keyword</description>
            <required>false</required>
        </param>
        <param>
            <name>page</name>
            <description>empty</description>
            <required>false</required>
        </param>
        <param>
            <name>pagesize</name>
            <description>empty</description>
            <required>false</required>
        </param>
        <param>
            <name>role</name>
            <description>list accounts of the project by role</description>
            <required>false</required>
        </param>
    </params>
    <responses>
        <response>
            <name>projectid</name>
            <description>the id of the project</description>
        </response>
        <response>
            <name>account</name>
            <description>the account name of the project&apos;s owner</description>
        </response>
        <response>
            <name>displaytext</name>
            <description>the displaytext of the project</description>
        </response>
        <response>
            <name>domain</name>
            <description>the domain name where the project belongs to</description>
        </response>
        <response>
            <name>domainid</name>
            <description>the domain id the project belongs to</description>
        </response>
        <response>
            <name>project</name>
            <description>the name of the project</description>
        </response>
        <response>
            <name>state</name>
            <description>the state of the project</description>
        </response>
    </responses>
</root>